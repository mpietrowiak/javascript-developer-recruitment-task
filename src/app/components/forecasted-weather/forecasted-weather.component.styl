.forecastedWeatherList {
  list-style: none;
  padding: 10px 0;
  li {
    float: left;
    margin: 10px;
  }
}

.forecastedWeather {
  text-decoration: none;
  color: #000;
  border: 1px solid #000;
  display: flex;
  width: 200px;
  height: 150px;
  align-items: center;
  justify-content: center;
  flex-direction: column;

  h4 {
    margin-top: 0;
  }
}

